{
  "expo": {
    "name": "한자단어장",
    "slug": "hanja-words",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hanjawords",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chad.hanjawords",
      "buildNumber": "1.0.1",
      "infoPlist": {
        "CFBundleDisplayName": "한자단어장",
        "CFBundleName": "한자단어장",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.chad.hanjawords",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-3195009493032065~6057854187"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "081d126c-f24b-4a08-bca3-37ed8ebc53e5"
      }
    }
  }
}
